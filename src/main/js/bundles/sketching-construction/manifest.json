{
  "name": "sketching-construction",
  "version": "1.0.0",
  "title": "Sketching Construction",
  "dependencies": {
    "sketching-tools": "^1.0.0",
    "snapping-manager": "^1.0.0"
  },
  "productName": "map.apps",
  "components": [
    {
      "name": "Config",
      "impl": "./SketchingConstructionModel",
      "provides": [
        "sketching-construction.SketchingConstructionModel"
      ],
      "propertiesConstructor": true,
      "properties": {
        "angle": 45,
        "angleModulus": 45,
        "angleTypeRelative": true,
        "planarLength": 10,
        "commaPlace": 2,
        "use": {
          "snap": true,
          "angle": false,
          "angleModulus": false,
          "planarLength": false
        },
        "tools": [
          "drawpolylinetool",
          "drawpolygontool"
        ]
      }
    },
    {
      "name": "SketchingConstruction",
      "provides": [
        "sketching-construction.SketchingConstruction",
        "sketching.Action"
      ],
      "references": [
        {
          "name": "_constructionModel",
          "providing": "sketching-construction.SketchingConstructionModel"
        },
        {
          "name": "_snappingManager",
          "providing": "snapping-manager.SnappingManager",
          "cardinality": "0..1"
        }
      ]
    },
    {
      "name": "SketchingConstructionInfo",
      "provides": [
        "sketching-construction.SketchingConstructionInfo",
        "sketching.Action"
      ],
      "references": [
        {
          "name": "_constructionModel",
          "providing": "sketching-construction.SketchingConstructionModel"
        }
      ]
    }
  ]
}
